#include<stdio.h>
int main(){
    int num;
    long long fact = 1;

    printf("\n=====Factorial Calculator=====\n");

    printf("\nEnter a number: ");
    scanf("%d",&num);

    if(num<0){
        printf("Factorial of Negative number is not defined.\n");
    }else{
        for(int i=1; i<=num; i++){
            fact = fact * i;
        }
        printf("Factorial of %d = %d\n",num, fact);
    }
    return 0;
}
